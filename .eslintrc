 {
   "root": true,
   "parser": "babel-eslint",
   "env": {
     "browser": true
   },
   "plugins": ["react"],
   "extends": "standard",
   "rules": {
     "react/jsx-uses-react": 1,
     "react/jsx-uses-vars": 1,
     "semi": 2,
     "arrow-parens": 0,
     "generator-star-spacing": 0,
     "no-return-assign": 0,
     "one-var": 0,
     "space-before-function-paren": 0,
     "no-useless-return": 0,
     "max-len": ["error", 120, 2]
   }
 }
